{
  "name": "openclaw-cn",
  "version": "0.1.0",
  "description": "中国商用个人 AI 助手 - 支持企业微信/钉钉/飞书",
  "type": "module",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js",
    "./plugin-sdk": "./dist/plugin-sdk/index.js",
    "./plugin-sdk/*": "./dist/plugin-sdk/*",
    "./cli-entry": "./openclaw.mjs"
  },
  "bin": {
    "openclaw": "./openclaw.mjs"
  },
  "files": [
    "dist/**",
    "extensions/**",
    "assets/**",
    "openclaw.mjs",
    "skills/**",
    "patches/**",
    "scripts/postinstall.js",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "start": "vite build && tsc -p tsconfig.electron.json && tsc -p tsconfig.preload.json && electron electron.cjs",
    "dev": "lsof -ti:18789 | xargs kill -9 2>/dev/null; lsof -ti:5173 | xargs kill -9 2>/dev/null; node openclaw.mjs gateway & vite",
    "build": "vite build && tsc -p tsconfig.electron.json && tsc -p tsconfig.preload.json",
    "dist": "pnpm build && electron-builder",
    "postinstall": "node scripts/postinstall.js"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "engines": {
    "node": ">=22.12.0"
  },
  "packageManager": "pnpm@10.23.0",
  "dependencies": {
    "@agentclientprotocol/sdk": "0.13.1",
    "@ant-design/icons": "^6.1.0",
    "@aws-sdk/client-bedrock": "^3.975.0",
    "@buape/carbon": "^0.14.0",
    "@clack/prompts": "^0.11.0",
    "@grammyjs/runner": "^2.0.3",
    "@grammyjs/transformer-throttler": "^1.2.1",
    "@homebridge/ciao": "^1.3.4",
    "@line/bot-sdk": "^10.6.0",
    "@lydell/node-pty": "1.2.0-beta.3",
    "@mariozechner/pi-agent-core": "0.49.3",
    "@mariozechner/pi-ai": "0.49.3",
    "@mariozechner/pi-coding-agent": "0.49.3",
    "@mariozechner/pi-tui": "0.49.3",
    "@mozilla/readability": "^0.6.0",
    "@noble/ed25519": "3.0.0",
    "@sinclair/typebox": "0.34.47",
    "@slack/bolt": "^4.6.0",
    "@slack/web-api": "^7.13.0",
    "@whiskeysockets/baileys": "7.0.0-rc.9",
    "ajv": "^8.17.1",
    "antd": "^6.2.2",
    "body-parser": "^2.2.2",
    "chalk": "^5.6.2",
    "chokidar": "^5.0.0",
    "chromium-bidi": "13.0.1",
    "cli-highlight": "^2.1.11",
    "commander": "^14.0.2",
    "croner": "^9.1.0",
    "detect-libc": "^2.1.2",
    "discord-api-types": "^0.38.38",
    "discord.js": "^14.25.1",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "file-type": "^21.3.0",
    "grammy": "^1.39.3",
    "hono": "^4.11.7",
    "jiti": "^2.6.1",
    "json5": "^2.2.3",
    "jszip": "^3.10.1",
    "linkedom": "^0.18.12",
    "long": "5.3.2",
    "markdown-it": "^14.1.0",
    "node-edge-tts": "^1.2.9",
    "osc-progress": "^0.3.0",
    "pdfjs-dist": "^5.4.530",
    "playwright-core": "1.58.0",
    "proper-lockfile": "^4.1.2",
    "qrcode-terminal": "^0.12.0",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.13.0",
    "sharp": "^0.34.5",
    "sqlite-vec": "0.1.7-alpha.2",
    "tar": "^7.5.7",
    "tslog": "^4.10.2",
    "undici": "^7.19.0",
    "ws": "^8.19.0",
    "yaml": "^2.8.2",
    "zod": "^4.3.6",
    "zustand": "^5.0.10"
  },
  "optionalDependencies": {
    "@napi-rs/canvas": "^0.1.88",
    "node-llama-cpp": "3.15.0"
  },
  "devDependencies": {
    "@lit-labs/signals": "^0.2.0",
    "@lit/context": "^1.1.6",
    "@mariozechner/mini-lit": "0.2.1",
    "@types/body-parser": "^1.19.6",
    "@types/express": "^5.0.6",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^25.0.10",
    "@types/proper-lockfile": "^4.1.4",
    "@types/qrcode-terminal": "^0.12.2",
    "@types/ws": "^8.18.1",
    "@typescript/native-preview": "7.0.0-dev.20260124.1",
    "@vitejs/plugin-react": "^5.1.2",
    "@vitest/coverage-v8": "^4.0.18",
    "docx-preview": "^0.3.7",
    "electron": "^31.0.0",
    "electron-builder": "^24.13.0",
    "lit": "^3.3.2",
    "lucide": "^0.563.0",
    "ollama": "^0.6.3",
    "oxfmt": "0.26.0",
    "oxlint": "^1.41.0",
    "oxlint-tsgolint": "^0.11.1",
    "quicktype-core": "^23.2.6",
    "rolldown": "1.0.0-rc.1",
    "signal-utils": "^0.21.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.18",
    "wireit": "^0.14.12"
  },
  "overrides": {
    "tar": "^7.5.7"
  },
  "pnpm": {
    "minimumReleaseAge": 2880,
    "overrides": {
      "@sinclair/typebox": "0.34.47",
      "hono": "^4.11.7",
      "tar": "^7.5.7",
      "fast-xml-parser": "^5.3.4"
    },
    "ignoredBuiltDependencies": [
      "electron"
    ]
  },
  "build": {
    "appId": "com.openclaw.cn",
    "productName": "OpenClaw CN",
    "directories": {
      "output": "release"
    },
    "extends": null,
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "dist",
        "to": "backend",
        "filter": [
          "**/*",
          "!**/*.map",
          "!electron/**"
        ]
      }
    ],
    "mac": {
      "category": "public.app-category.productivity",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "target": [
        "nsis",
        "portable"
      ]
    },
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    }
  },
  "vitest": {
    "coverage": {
      "provider": "v8",
      "reporter": [
        "text",
        "lcov"
      ],
      "thresholds": {
        "lines": 70,
        "functions": 70,
        "branches": 70,
        "statements": 70
      },
      "include": [
        "src/**/*.ts"
      ],
      "exclude": [
        "src/**/*.test.ts"
      ]
    },
    "include": [
      "src/**/*.test.ts"
    ],
    "exclude": [
      "dist/**",
      "apps/macos/**",
      "apps/macos/.build/**",
      "**/vendor/**",
      "apps/macos/.build/**",
      "dist/OpenClaw.app/**"
    ]
  }
}
